<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>VerProfesores</servlet-name>
        <servlet-class>Servlets.VerProfesores</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InfoProfesor</servlet-name>
        <servlet-class>Servlets.InfoProfesor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Inscrito</servlet-name>
        <servlet-class>Servlets.Inscrito</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccedeCuenta</servlet-name>
        <servlet-class>Servlets.AccedeCuenta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerificarProfesores</servlet-name>
        <servlet-class>Servlets.VerificarProfesores</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BorrarProfesor</servlet-name>
        <servlet-class>Servlets.BorrarProfesor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AceptarProfesor</servlet-name>
        <servlet-class>Servlets.AceptarProfesor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InscribirCursos</servlet-name>
        <servlet-class>Servlets.InscribirCursos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerCursos</servlet-name>
        <servlet-class>Servlets.VerCursos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerNivel</servlet-name>
        <servlet-class>Servlets.VerNivel</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VerProfesores</servlet-name>
        <url-pattern>/VerProfesores</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Inscrito</servlet-name>
        <url-pattern>/Inscrito</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerificarProfesores</servlet-name>
        <url-pattern>/VerificarProfesores</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InscribirCursos</servlet-name>
        <url-pattern>/InscribirCursos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InfoProfesor</servlet-name>
        <url-pattern>/InfoProfesor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccedeCuenta</servlet-name>
        <url-pattern>/AccedeCuenta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BorrarProfesor</servlet-name>
        <url-pattern>/BorrarProfesor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AceptarProfesor</servlet-name>
        <url-pattern>/AceptarProfesor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerCursos</servlet-name>
        <url-pattern>/VerCursos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerNivel</servlet-name>
        <url-pattern>/VerNivel</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
